    },
    {
      "before": ["h", "h"],
      "after": ["left", "<C-o>"]
    },
    {
      "before": ["i", "Enter"],
      "after": ["<Esc>", "o"]
    },
    {
      "before": ["I", "Enter"],
      "after": ["<Esc>", "O"]
    }
  ],
