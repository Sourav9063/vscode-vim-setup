      "textMateRules": [
        {
          "name": "Function argument",
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": "",
            "foreground": "#6688CC"
          }
        },
        {
          "name": "Class name",
          "scope": [
            "entity.name.class",
            "entity.name.type",
            "entity.name.namespace",
            "entity.name.scope-resolution"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#9966b8"
          }
        }
      ]
    }
  },

